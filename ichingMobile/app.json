{
  "expo": {
    "name": "ichingMobile",
    "slug": "ichingMobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/icon.png",
    "scheme": "com.bmj1988.ichingMobile",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./src/assets/diamondSplash.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.bmj1988.ichingMobile"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/black-diamond.png",
        "backgroundColor": "#000000"
      },
      "package": "com.bmj1988.ichingMobile"
    },
    "web": {
      "favicon": "./src/assets/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "extra": {
      "eas": {
        "projectId": "3d942656-a6e8-42c2-a7ac-5bf3f88f4fb5"
      }
    }
  }
}
